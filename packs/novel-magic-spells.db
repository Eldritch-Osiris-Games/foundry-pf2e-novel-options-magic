{"name":"Warp Tradition","type":"spell","img":"systems/pf2e/icons/default-icons/spell.svg","system":{"description":{"value":"<p>You force your magical energy to rapidly shift to another tradition. Choose a tradition you are not currently locked into, you become locked into that tradition.</p>"},"source":{"value":"Novel Options: Magic"},"traits":{"value":["hb_exploit","hb_theurge"],"rarity":"uncommon","custom":""},"rules":[],"slug":null,"schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-21T16:31:37.938-05:00","version":{"schema":null,"foundry":"10.288","system":"4.2.6"}}},"level":{"value":1},"spellType":{"value":"utility"},"category":{"value":"focus"},"traditions":{"value":[]},"school":{"value":"abjuration"},"components":{"focus":false,"material":false,"somatic":true,"verbal":false},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":"","areaType":""},"time":{"value":"1"},"duration":{"value":""},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":""},"hasCounteractCheck":{"value":false},"primarycheck":{"value":""},"secondarycheck":{"value":""},"secondarycasters":{"value":""}},"effects":[],"ownership":{"default":0,"gKRAixZcnc2H4VRx":3},"flags":{"core":{"sourceId":"Item.4h5QXzaXXge9jKGN"}},"_stats":{"systemId":"pf2e","systemVersion":"4.2.6","coreVersion":"10.288","createdTime":1666387897952,"modifiedTime":1666388009828,"lastModifiedBy":"gKRAixZcnc2H4VRx"},"folder":null,"sort":0,"_id":"n10nukDjQzNHlT18"}
